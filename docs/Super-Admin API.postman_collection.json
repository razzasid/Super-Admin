					"name": "login",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"const res = pm.response.json();\r",
									"\r",
									"if (res.token) {\r",
									"  pm.environment.set(\"token\", res.token);\r",
									"}\r",
									""
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
					],
					"request": {
						"auth": {
							"type": "bearer",
							"bearer": [
								{
									"key": "token",
									"value": "{{token}}",
									"type": "string"
								}
							]
						},
						"method": "POST",
						"header": [],
						"body": {
							"mode": "raw",
							"raw": "{ \"email\": \"superadmin@example.com\", \"password\": \"Test1234!\" }",
							"options": {
								"raw": {
									"language": "json"
								}
							}
						},
						"url": {
							"raw": "{{base_url}}api/v1/auth/login",
							"host": [
								"{{base_url}}api"
							],
							"path": [
								"v1",
								"auth",
								"login"
							]
						}
					},
					"response": []
				}
			]
